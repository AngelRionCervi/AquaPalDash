{
  "name": "aquapaldash",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "npm run cert && NODE_ENV='development' concurrently \"npm:dev-front\" \"npm:serve\"",
    "dev-front": "vite dev --mode development --host",
    "cert": "node checkCert.js",
    "serve": " NODE_EXTRA_CA_CERTS=\"./cert/rootCA.pem\" tsx ./server/index.ts",
    "serveProd": "NODE_ENV='production' npm run serve",
    "build": "vite build --mode production",
    "preview": "vite preview --mode production",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "svelte-check": "svelte-check"
  },
  "devDependencies": {
    "@poppanator/sveltekit-svg": "^5.0.0-svelte5.3",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/eslint": "^8.56.7",
    "@types/web-bluetooth": "^0.0.20",
    "@types/ws": "^8.5.12",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0",
    "globals": "^15.0.0",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "sass": "^1.77.4",
    "svelte": "^5.0.0-next.151",
    "svelte-check": "^3.8.6",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0-alpha.20",
    "vite": "^5.0.3"
  },
  "type": "module",
  "dependencies": {
    "@sveltejs/adapter-node": "^5.2.2",
    "@types/express": "^4.17.21",
    "apexcharts": "^3.51.0",
    "bcrypt": "^5.1.1",
    "concurrently": "^8.2.2",
    "express": "^4.19.2",
    "short-unique-id": "^5.2.0",
    "tsx": "^4.17.0",
    "vite-plugin-mkcert": "^1.17.6",
    "ws": "^8.18.0"
  }
}
