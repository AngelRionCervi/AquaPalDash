{
  "name": "aquapaldash",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "npm run cert && NODE_ENV='development' concurrently \"npm:dev-front\" \"npm:serve\"",
    "dev-front": "vite dev --mode development --host",
    "cert": "node checkCert.js",
    "serve": " NODE_EXTRA_CA_CERTS=\"./cert/rootCA.pem\" tsx ./server/index.ts",
    "serveProd": "NODE_ENV='production' npm run serve",
    "build": "vite build --mode production",
    "preview": "vite preview --mode production",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "svelte-check": "svelte-check"
  },
  "devDependencies": {
    "@poppanator/sveltekit-svg": "^5.0.0",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/kit": "^2.8.2",
    "@sveltejs/vite-plugin-svelte": "^4.0.1",
    "@types/bcrypt": "^5.0.2",
    "@types/eslint": "^9.6.1",
    "@types/web-bluetooth": "^0.0.20",
    "@types/ws": "^8.5.13",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.0",
    "globals": "^15.12.0",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.3.2",
    "sass": "^1.81.0",
    "svelte": "^5.2.7",
    "svelte-check": "^4.1.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^5.4.11"
  },
  "type": "module",
  "dependencies": {
    "@sveltejs/adapter-node": "^5.2.9",
    "@types/express": "^5.0.0",
    "apexcharts": "^4.0.0",
    "bcrypt": "^5.1.1",
    "concurrently": "^9.1.0",
    "express": "^4.21.1",
    "short-unique-id": "^5.2.0",
    "tsx": "^4.19.2",
    "vite-plugin-mkcert": "^1.17.6",
    "ws": "^8.18.0"
  }
}
