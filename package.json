{
	"name": "aquapaldash",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --mode development",
    "devProd": "NODE_ENV='production' vite dev --mode production",
		"build": "vite build --mode production",
		"preview": "vite preview --mode production",
